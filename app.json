{
  "expo": {
    "name": "Buy European",
    "slug": "buy-european",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "buyeuropean",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow Buy European to access your camera to scan product barcodes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}